{
  "name": "micro-truffle",
  "version": "0.1.0",
  "description": "Truffle boilerplate code",
  "license": "Apache-2.0",
  "author": "Akagi201 <akagi201@gmail.com>",
  "scripts": {
    "build": "truffle compile",
    "develop": "truffle develop",
    "migrate:moonbase-alpha": "truffle migrate --network moonbase",
    "migrate:moonbeam-dev": "truffle migrate --network dev --reset",
    "moonbeam-install": "truffle run moonbeam install",
    "moonbeam-pause": "truffle run moonbeam pause",
    "moonbeam-remove": "truffle run moonbeam remove",
    "moonbeam-start": "truffle run moonbeam start",
    "moonbeam-status": "truffle run moonbeam status",
    "moonbeam-stop": "truffle run moonbeam stop",
    "moonbeam-unpause": "truffle run moonbeam unpause",
    "prettier": "prettier '**/*.{json,sol,md}' --check",
    "prettier-fix": "prettier '**/*.{json,sol,md}' --write",
    "solhint": "solhint 'contracts/**/*.sol'",
    "solhint-fix": "solhint 'contracts/**/*.sol' --fix",
    "slither": "slither .",
    "sort-package": "sort-package-json",
    "test": "truffle test"
  },
  "devDependencies": {
    "@openzeppelin/contracts": "^4.6.0",
    "@truffle/hdwallet-provider": "2.0.8",
    "dotenv": "^16.0.3",
    "moonbeam-truffle-plugin": "^1.4.0",
    "prettier": "^2.6.2",
    "prettier-plugin-solidity": "^1.0.0-beta.19",
    "solhint": "^3.3.7",
    "solidity-coverage": "^0.7.21",
    "sort-package-json": "^1.57.0",
    "truffle": "^5.5.12",
    "truffle-plugin-verify": "^0.5.25"
  },
  "engines": {
    "node": ">= 16.14 <17"
  },
  "volta": {
    "node": "16.14.2"
  }
}
